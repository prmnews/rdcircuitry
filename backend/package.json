{
  "name": "rdcircuitry-backend",
  "version": "1.0.0",
  "description": "Backend API for RD Circuitry",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:auth": "jest src/tests/login.test.ts",
    "test:auth:verbose": "jest src/tests/login.test.ts --verbose",
    "test:timer": "jest src/tests/timer.test.ts",
    "test:timer:verbose": "jest src/tests/timer.test.ts --verbose",
    "hydrate": "npx ts-node src/scripts/hydrate-all.ts",
    "hydrate:users": "npx ts-node src/scripts/hydrate-users.ts",
    "hydrate:countries": "npx ts-node src/scripts/hydrate-countries.ts",
    "hydrate:apikeys": "npx ts-node src/scripts/hydrate-apiKeys.ts",
    "hydrate:state": "npx ts-node src/scripts/hydrate-state.ts",
    "hydrate:messagetimer": "npx ts-node src/scripts/hydrate-messageTimer.ts",
    "hydrate:all": "npx ts-node src/scripts/hydrate-all.ts",
    "update:state": "npx ts-node src/scripts/update-state.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.7.1",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.1",
    "socket.io": "^4.8.1",
    "twitter-api-v2": "^1.22.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^22.15.21",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "4.0.0-beta1",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2"
  }
}
